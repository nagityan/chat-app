<div class="side_header">
  <div class="user_name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="new_room">
    <a href="#">チャットを作成する</a>
  </div>
</div>
<div class="side_body">
  <%current_user.rooms.each do|room|%>
    <div class="room_name">
      <%= link_to room.name, room_messages_path(room) %>
    </div>
  <%end%>
</div>
